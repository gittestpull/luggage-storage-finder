# 서버 설정
PORT=3000
NODE_ENV=development

# MongoDB 설정
# MongoDB 설정
# Docker 컨테이너 내부에서 실행 시: MONGO_URI=mongodb://mongo:27017/luggage-storage
# Docker 컨테이너 외부에서 실행 시: MONGO_URI=mongodb://localhost:27017/luggage-storage
MONGO_URI=mongodb://localhost:27017/luggage-storage

# JWT 비밀 키
JWT_SECRET=your_jwt_secret_key_here

# Google Maps API 키
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# 파일 업로드 설정
UPLOAD_DIR=public/uploads

# 결제 시스템 키 (실제 구현 시 필요)
PAYMENT_API_KEY=your_payment_api_key
PAYMENT_SECRET=your_payment_secret_key

# 이메일 발송 설정 (Nodemailer)
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_password
ADMIN_EMAIL=admin@example.com
